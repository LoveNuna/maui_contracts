{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "base_denom",
    "collateral_token",
    "market_contract",
    "overseer_contract",
    "reward_contract"
  ],
  "properties": {
    "base_denom": {
      "description": "Expected reward denom. If bAsset reward is not same with it, we try to convert the reward to the `base_denom`.",
      "type": "string"
    },
    "collateral_token": {
      "description": "bAsset token address",
      "allOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        }
      ]
    },
    "market_contract": {
      "description": "market contract address",
      "allOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        }
      ]
    },
    "overseer_contract": {
      "description": "overseer contract address",
      "allOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        }
      ]
    },
    "reward_contract": {
      "description": "bAsset rewrad contract",
      "allOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        }
      ]
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    }
  }
}
