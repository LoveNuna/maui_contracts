{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "config"
      ],
      "properties": {
        "config": {
          "type": "object"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "liquidation_amount"
      ],
      "properties": {
        "liquidation_amount": {
          "type": "object",
          "required": [
            "borrow_amount",
            "borrow_limit",
            "collateral_prices",
            "collaterals",
            "stable_denom"
          ],
          "properties": {
            "borrow_amount": {
              "$ref": "#/definitions/Uint128"
            },
            "borrow_limit": {
              "$ref": "#/definitions/Uint128"
            },
            "collateral_prices": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Decimal"
              }
            },
            "collaterals": {
              "type": "array",
              "items": {
                "type": "array",
                "items": [
                  {
                    "$ref": "#/definitions/HumanAddr"
                  },
                  {
                    "$ref": "#/definitions/Uint128"
                  }
                ],
                "maxItems": 2,
                "minItems": 2
              }
            },
            "stable_denom": {
              "type": "string"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    },
    "Uint128": {
      "type": "string"
    }
  }
}
