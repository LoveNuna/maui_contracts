{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "anyOf": [
    {
      "description": "Query distribution params to overseer contract",
      "type": "object",
      "required": [
        "distribution_params"
      ],
      "properties": {
        "distribution_params": {
          "type": "object"
        }
      }
    },
    {
      "description": "Query epoch state to market contract",
      "type": "object",
      "required": [
        "epoch_state"
      ],
      "properties": {
        "epoch_state": {
          "type": "object"
        }
      }
    },
    {
      "description": "Query borrow amount to market contract",
      "type": "object",
      "required": [
        "loan_amount"
      ],
      "properties": {
        "loan_amount": {
          "type": "object",
          "required": [
            "block_height",
            "borrower"
          ],
          "properties": {
            "block_height": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "borrower": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    },
    {
      "description": "Query oracle price to oracle contract",
      "type": "object",
      "required": [
        "price"
      ],
      "properties": {
        "price": {
          "type": "object",
          "required": [
            "base",
            "quote"
          ],
          "properties": {
            "base": {
              "type": "string"
            },
            "quote": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "description": "Query borrow rate to interest model contract",
      "type": "object",
      "required": [
        "borrow_rate"
      ],
      "properties": {
        "borrow_rate": {
          "type": "object",
          "required": [
            "market_balance",
            "total_liabilities",
            "total_reserve"
          ],
          "properties": {
            "market_balance": {
              "$ref": "#/definitions/Uint256"
            },
            "total_liabilities": {
              "$ref": "#/definitions/Decimal256"
            },
            "total_reserve": {
              "$ref": "#/definitions/Decimal256"
            }
          }
        }
      }
    },
    {
      "description": "Query borrow limit to overseer contract",
      "type": "object",
      "required": [
        "borrow_limit"
      ],
      "properties": {
        "borrow_limit": {
          "type": "object",
          "required": [
            "borrower"
          ],
          "properties": {
            "borrower": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    },
    {
      "description": "Query liquidation amount to liquidation model contract",
      "type": "object",
      "required": [
        "liquidation_amount"
      ],
      "properties": {
        "liquidation_amount": {
          "type": "object",
          "required": [
            "borrow_amount",
            "borrow_limit",
            "collateral_prices",
            "collaterals",
            "stable_denom"
          ],
          "properties": {
            "borrow_amount": {
              "$ref": "#/definitions/Uint256"
            },
            "borrow_limit": {
              "$ref": "#/definitions/Uint256"
            },
            "collateral_prices": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Decimal256"
              }
            },
            "collaterals": {
              "type": "array",
              "items": {
                "type": "array",
                "items": [
                  {
                    "$ref": "#/definitions/HumanAddr"
                  },
                  {
                    "$ref": "#/definitions/Uint256"
                  }
                ],
                "maxItems": 2,
                "minItems": 2
              }
            },
            "stable_denom": {
              "type": "string"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Decimal256": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal256(1_000_000_000_000_000_000) == 1.0 The greatest possible value that can be represented is 115792089237316195423570985008687907853269984665640564039457.584007913129639935 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    },
    "Uint256": {
      "type": "string"
    }
  }
}
